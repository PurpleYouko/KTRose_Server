        for(int j=1;j<100;j++)
        {
            if(GServer->WPList[mapid][j].WPType != 0)
            GServer->DB->QExecute("INSERT INTO list_waypoints (mapid,wpnum,mapx,mapy,wptype) VALUES(%i,%i,%f,%f,%i)",
