		else if((otherclient->CharInfo->Job - 0) == 422) { jobname = "Artisan"; }
		else { jobname = "Unknown"; }

		BEGINPACKET(pak, 0x702);

		sprintf(buffer, "Info about <%s>" , otherclient->CharInfo->charname);
